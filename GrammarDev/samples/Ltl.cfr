abstract Bla
  A +
    B -> int
    A_Const -> int // constant value
    xor C
      C1
      C2
    xor D
      D1
      D2
      D3
      
  E -> int
  M ?
  N ?
  [ B >= 0
    C >= 0
    D <= A_Const
    A_Const > 0 && A_Const < 1000
  ]
  
  // lines below -> behaviour 
  [ M => X(N) ]
  
  [ X(N) U X(M)]
  
  [ G (C1 => F(D3)) ]
  
  [ C2 U D2]
  
  [ C2 W D1   ]
  
  [ (M && N) => X(D3)  ]
  
  [ D3 R C2]

  
concrete: Bla
  [B = 7
   A_Const = 100
  ]
  
  
